{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^/"
    },
    {
      "pattern": "^\\.\\./"
    },
    {
      "pattern": "^\\.\/"
    },
    {
      "pattern": "https://github.com/vsmenu/vsmenu-(api|delivery-web|desktop|mobile-waiter|mobile-deliverer|design-system|infrastructure)"
    },
    {
      "pattern": "https://github.com/vsmenu/vsmenu-docs/(discussions|compare|releases)"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://github.com/"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 302, 304, 999]
}

