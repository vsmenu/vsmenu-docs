{
  "name": "vsmenu-docs",
  "version": "1.0.0",
  "type": "module",
  "description": "Documentação Central - Toda a documentação técnica e arquitetural do projeto VSmenu 2.0",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules --ignore docs/.vitepress",
    "lint:md:fix": "markdownlint '**/*.md' --ignore node_modules --ignore docs/.vitepress --fix",
    "check-links": "markdown-link-check docs/**/*.md --config .markdown-link-check.json",
    "test": "npm run docs:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsmenu/vsmenu-docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vsmenu/vsmenu-docs/issues"
  },
  "homepage": "https://github.com/vsmenu/vsmenu-docs#readme",
  "devDependencies": {
    "markdown-link-check": "^3.12.2",
    "markdownlint-cli": "^0.42.0",
    "mermaid": "^11.12.1",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.24"
  }
}
